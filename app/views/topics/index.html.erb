<div class="">
  <h1>Breakout</h1>
</div>

<% if user_signed_in? %>
  <%= render 'new' %>

  <%= @topics.each do |top| %>
    <ol>
      <li><%= top.title %></li>
    </ol>
  <% end %>
<% else %>
  <%= link_to "Sign Up", new_user_registration_path %>
  <%= link_to "Log in", new_user_session_path %>
<% end %>